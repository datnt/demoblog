{
    "status": {
      "code": "success",
      "message":"Link is converted!"
    },
    "data": <% if  @city_statistics.data.rows.size > 0 %>
            [
            ["City", "Visits"],
            <% @city_statistics.data.rows.each_with_index do |rows,i| %>
              [
              <% rows.each_with_index do |i,index| %>
                <% if index == 1  %>
                  "<%= i %>",
                <% elsif index == 2 %>
                  <%= i %>
                <% end %>
              <% end %>
              ]    
              <% if i+1 < @city_statistics.data.rows.size %>
                ,
              <% end %>
            <% end %>
            ]
          <% end %>     
}